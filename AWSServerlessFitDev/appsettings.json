{
  "Logging": {
    "Region": "eu-central-1",
    "LogGroup": "GymnectAppLogs",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Warning",
      "Microsoft": "Warning",
      "Quartz": "Warning"
    },
    "Console": {
      "DisableColors": true
    }

  }

}